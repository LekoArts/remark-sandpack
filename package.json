{
	"name": "@lekoarts/remark-sandpack",
	"type": "module",
	"version": "0.0.1",
	"packageManager": "pnpm@10.11.0",
	"description": "Author Sandpack examples in MDX",
	"author": "LekoArts",
	"license": "MIT",
	"homepage": "https://github.com/LekoArts/remark-sandpack#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LekoArts/remark-sandpack.git"
	},
	"bugs": {
		"url": "https://github.com/LekoArts/remark-sandpack/issues"
	},
	"keywords": [
		"remark",
		"sandpack",
		"MDX"
	],
	"sideEffects": false,
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"typecheck": "tsc --noEmit",
		"changeset": "changeset",
		"version": "changeset version",
		"version:ci": "changeset version && pnpm install --lockfile-only",
		"release": "pnpm build && changeset publish"
	},
	"dependencies": {
		"estree-util-value-to-estree": "^3.4.0",
		"unist-util-visit": "^5.0.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.14.1",
		"@changesets/changelog-github": "^0.5.1",
		"@changesets/cli": "^2.29.4",
		"@types/mdast": "^4.0.4",
		"@types/node": "^24.0.3",
		"eslint": "^9.28.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-format": "^1.0.1",
		"mdast-util-mdx-jsx": "^3.2.0",
		"remark": "^15.0.1",
		"remark-mdx": "^3.1.0",
		"tsdown": "^0.12.8",
		"typescript": "^5.8.3",
		"vitest": "^3.2.3"
	}
